{
  "name": "managementbackendgraphql",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "dev": "npm run lint && nodemon",
    "start": "node /src/app.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "seed:generate": "npx sequelize seed:generate --name",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "migrations:generate": "npx sequelize-cli migration:generate --name",
    "migrations:run": "npx sequelize-cli db:migrate",
    "migrations:revert": "npx sequelize-cli db:migrate:undo",
    "migrations:delete": "npx sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@graphql-tools/load-files": "^7.0.0",
    "@types/sequelize": "^4.28.19",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-openid-connect": "^2.12.1",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.0",
    "@types/validator": "^13.7.14",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "mysql2": "^3.7.1",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
