{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2022",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src",
      "paths": {
        "@models/*":["models/*"],
        "@libs/*":["libs/*"],
        "@config/*":["config/*"],
        "@services/*":["services/*"]
      }
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }