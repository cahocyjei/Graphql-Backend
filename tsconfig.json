{
    "compilerOptions": {
      "module": "CommonJS",
      "target": "ESNext",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src",
      "paths": {
        "@libs/*":["libs/*"],
        "@services/*":["services/*"],
      }
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }